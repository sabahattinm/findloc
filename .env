# API Keys for FindLoc App
# Copy this file to .env and add your actual API keys

# Google Maps API Key
# Get from: https://console.cloud.google.com/
GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# OpenWeatherMap API Key
# Get from: https://openweathermap.org/api
OPENWEATHER_API_KEY=YOUR_OPENWEATHER_API_KEY_HERE

# Webcams.travel API Key
# Get from: https://www.webcams.travel/api/
WEBCAMS_TRAVEL_API_KEY=YOUR_WEBCAMS_TRAVEL_API_KEY_HERE

# Security Camera API Key
# Get from your security camera provider
SECURITY_CAMERA_API_KEY=YOUR_SECURITY_CAMERA_API_KEY_HERE

# Traffic Camera API Key
# Get from traffic management system
TRAFFIC_CAMERA_API_KEY=YOUR_TRAFFIC_CAMERA_API_KEY_HERE

# Tourism Board API Key
# Get from tourism board or booking services
TOURISM_API_KEY=YOUR_TOURISM_API_KEY_HERE

# Weather Station API Key
# Get from weather service provider
WEATHER_STATION_API_KEY=YOUR_WEATHER_STATION_API_KEY_HERE
